<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/rootFragment">

    <fragment
        android:id="@+id/rootFragment"
        android:name="com.egorprok.ancientphilosophers.RootFragment"
        android:label="fragment_root"
        tools:layout="@layout/fragment_root" >
        <action
            android:id="@+id/action_rootFragment_to_vityaFragment"
            app:destination="@id/vityaFragment" />
        <action
            android:id="@+id/action_rootFragment_to_volodyaFragment"
            app:destination="@id/volodyaFragment" />
    </fragment>
    <fragment
        android:id="@+id/vityaFragment"
        android:name="com.egorprok.ancientphilosophers.VityaFragment"
        android:label="fragment_vitya"
        tools:layout="@layout/fragment_vitya" />
    <fragment
        android:id="@+id/volodyaFragment"
        android:name="com.egorprok.ancientphilosophers.VolodyaFragment"
        android:label="fragment_volodya"
        tools:layout="@layout/fragment_volodya" />
</navigation>